{
  "name": "jonathan-yeong-personal-site",
  "version": "2.1.0",
  "description": "Jonathan Yeong's personal site - forked off 11ty base blog",
  "scripts": {
    "dev:eleventy": "eleventy --serve",
    "dev:postcss": "postcss src/assets/styles/main.css -d dist/assets/styles --config ./postcss.config.js -w",
    "prod:postcss": "postcss src/assets/styles/main.css -d dist/assets/styles --config ./postcss.config.js",
    "prod:eleventy": "ELEVENTY_ENV=production eleventy",
    "clean": "rimraf dist",
    "start": "npm-run-all clean --parallel dev:*",
    "build": "NODE_ENV=production npm-run-all clean --parallel prod:*"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jonathanyeong/personal-website-2020"
  },
  "author": {
    "name": "Jonathan Yeong",
    "email": "hello@jonathanyeong.com",
    "url": "https://jonathanyeong.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jonathanyeong/personal-website-2020/issues"
  },
  "homepage": "https://github.com/jonathanyeong/personal-website-2020#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "css-loader": "^5.0.1",
    "luxon": "^1.21.3",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.2.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.1",
    "postcss-cli": "^8.3.1",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^2.7.1"
  },
  "dependencies": {
    "autoprefixer": "^10.1.0",
    "striptags": "^3.1.1",
    "tailwindcss": "^2.0.2"
  }
}
